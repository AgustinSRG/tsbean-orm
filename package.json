    "lint"
  ],
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
